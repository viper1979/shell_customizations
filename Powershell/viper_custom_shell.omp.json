{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#b8841c",
            "style": "plain",
            "template": "\u26a1",
            "type": "os"
          },          
          {
            "background": "#2b2b2b",
            "foreground": "#93acaa",
            "properties": {
              "style": "roundrock",
              "threshold": 0
            },
            "leading_diamond": "\ue0b6",
            "trailing_diamond": "\uE0BC",
            "style": "diamond",
            "template": "\ufa1e{{ .FormattedMs }}\u2800",
            "type": "executiontime"
          },        
          {
            "style": "plain",
            "template": "\u2800",
            "type": "text"
          },      
          {
            "type": "exit",
            "style": "diamond",
            "foreground": "#000000",
            "background": "#9c2525",
            "background_templates": ["{{ if gt .Code 0 }}#9c2525{{ end }}"],
            "leading_diamond": "\uE0BA",
            "trailing_diamond": "\uE0BC",
            "template": "\uf12a {{.Code}}",
            "properties": {
              "always_enabled": false
            }
          },          
          {
            "type": "exit",
            "style": "diamond",
            "template": "\u2800",
            "properties": {
              "always_enabled": false
            }
          }, 
          {
            "background": "#2b2b2b",
            "foreground": "#65aff4",
            "properties": {
              "folder_separator_icon": "\\",
              "style": "agnoster_short",
              "max_depth": 3
            },
            "leading_diamond": "\uE0BA",
            "trailing_diamond": "\ue0b0",
            "style": "diamond",
            "template": " \uf07b {{ .Path }} ",
            "type": "path"
          },
          {
            "foreground": "#011627",
            "background": "#6c8936",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#d2c33f{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
              "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
              "{{ if gt .Behind 0 }}#B388FB{{ end }}"
            ],            
            "properties": {
              "branch_icon": "\ue725 ",
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": false,
              "fetch_worktree_count": true
            },
            "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",            
            "leading_diamond": "<transparent,background>\uE0B0</>",
            "trailing_diamond": "\ue0b0",
            "style": "diamond",
            "type": "git"
          },
          {
            "type": "upgrade",
            "style": "diamond",
            "foreground": "#111111",
            "background": "#FFD664",
            "leading_diamond": "<transparent,background>\uE0B0</>",
            "trailing_diamond": "\ue0b0",
            "template": "\uf1da"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "type": "battery",
            "style": "diamond",
            "trailing_diamond": "<#8b8b8b>\ue621</>",            
            "foreground": "#606060",
            "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}% ",
            "properties": {
              "discharging_icon": "\uf242\u2800 ",
              "charging_icon": "\uE234 ",
              "charged_icon": "\uE22F "
            }
          },  
          // {
          //   "type": "text",
          //   "style": "diamond",
          //   "foreground": "#606060",
          //   "trailing_diamond": "<#8b8b8b>\ue621</>",
          //   "template": "\uf242\u2800 65% \uf588 "
          //   // f589 f0e7
          // },
          {
            "foreground": "#606060",
            "properties": {
                "time_format": "15:04:05"
            },
            "style": "plain",
            // "template": "\uf43a [{{ .CurrentDate | date .Format }}]",
            "template": " {{ .CurrentDate | date .Format }}",
            "type": "time"
          }          
        ],
        "type": "prompt"
      },      
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#cd5e42",
            "style": "plain",
            "template": "\ue3bf ",
            "type": "root"
          },
          {
            "foreground": "#c7c7c7",
            "style": "plain",
            "template": "# ",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "version": 2
  }
  